{
  "name": "meli",
  "version": "1.0.0-beta.20",
  "description": "Authentication and authorization service. Never write auth again.",
  "repository": {
    "type": "git",
    "url": "https://github.com/id6/id6"
  },
  "scripts": {
    "install": "npm i --prefix server && npm i --prefix ui",
    "start": "npm-run-all --parallel start:server start:ui",
    "start:server": "npm run start --prefix server",
    "start:ui": "npm run start --prefix ui",
    "build": "npm run build:server && npm run build:ui",
    "build:server": "npm run build --prefix server",
    "build:ui": "npm run build --prefix ui",
    "test": "npm run test:ui",
    "test:ui": "npm run test --prefix server",
    "lint": "npm run lint --prefix server"
  },
  "keywords": [
    "meli",
    "getmeli",
    "webdev",
    "jamstack",
    "static",
    "site",
    "deploy",
    "react"
    "angular"
    "vue",
    "nodejs"
  ],
  "author": "Geoffroy Empain",
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "semantic-release": "^17.4.2",
    "semantic-release-license": "^1.0.2"
  },
  "dependencies": {}
}
